{{#> layout }}
	<section class="container-fluid">
		<div class="container">
			<div class="row">
				{{#each htmlWebpackPlugin.options.data.categories.data }}
					<div class="col-12 col-sm-6 col-md-4">
					{{> l-media 
						title = this.title
						content = this.content
						customClasses = ''
						image = this.image
					}}
				</div>
				{{/each }}
			</div>
		</div>
	</section>
	<article class="container-fluid">
		<div class="container">
			<div class="row">
				<p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Deleniti cum tenetur voluptatibus nemo at dolorum a quos quibusdam quasi obcaecati natus, nesciunt, consequuntur quia delectus earum veniam facilis cumque alias!</p>
			</div>
		</div>
	</article>
	<article class="container">
		<div class="row">
			<div class="col-12 col-sm-6">
				<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d5986.78381754258!2d2.1378719249468!3d41.387294575484866!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0xce3cb4440aafdbf4!2sMIREIA+PODOLOGIA+LES+CORTS!5e0!3m2!1ses!2ses!4v1516268873888" width="100%" height="350px" frameborder="0" style="border:0" allowfullscreen></iframe>
				<div></div>
			</div> 
			<div class="col-12 col-sm-6">
				{{!> l-media 
						title = htmlWebpackPlugin.options.data.
						content = htmlWebpackPlugin.options.data.
						customClasses = ''
						image = htmlWebpackPlugin.options.data.
				}}
			</div>
		</div>
	</article>
{{/layout}}