<div class="alert {{ customClasses }}" role="alert">
    {{#if dismissible}}
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    {{/if}}
    {{#if heading }}
    	<h4 class="alert-heading">{{ heading }}</h4>
    {{/if}}
    {{{ content }}}
</div>